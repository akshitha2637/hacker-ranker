
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
char ch,s;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
        scanf("%[^\n]c",&ch);
        printf("%c",ch);
        scanf("%s",s);
        printf("%s",s);
        
    return 0;
}
